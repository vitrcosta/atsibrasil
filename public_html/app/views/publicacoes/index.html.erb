    <% @herbologia.each do |f| %>
      <% @herbologia.each do |f| %>
      <p><%= f.descricao.html_safe %></p>
      <% end %>
     <% end %>
    </div>
    <div class="parallax-publicacoes" data-parallax="scroll" data-image-src="/img/agronomia.jpg">
      <div class="container">
        <h1>AGRONOMIA</h1>
      </div>
    </div>
    <div class="container publicacoes">
    <% @agronomia.each do |f| %>
      <% @agronomia.each do |f| %>
      <p><%= f.descricao.html_safe %></p>
      <% end %>>
     <% end %>>
    </div>
</section>
