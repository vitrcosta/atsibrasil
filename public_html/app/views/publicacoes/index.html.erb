      <div class="container">
        <h1>ENTOMOLOGIA</h1>
      </div>
    </div>
    <div class="container publicacoes">
    <% unless @entomologia.nil? %>
    <% @entomologia.each do |f| %>
    <p><%= f.descricao.html_safe %></p>
    <% end %>
    <% end %>
    </div>
    <div class="parallax-publicacoes" data-parallax="scroll" data-image-src="/img/herbologia.jpg">
      <div class="container">
        <h1>HERBOLOGIA</h1>
      </div>
    </div>
